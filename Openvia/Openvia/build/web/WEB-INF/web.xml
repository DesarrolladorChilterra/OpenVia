<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>VaadinApplication</servlet-name>
        <servlet-class>com.vaadin.terminal.gwt.server.ApplicationServlet</servlet-class>
        <init-param>
            <param-name>application</param-name>
            <param-value>app.MyApplication</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>TraePDF</servlet-name>
        <servlet-class>servlets.TraePDF</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IniciaApp</servlet-name>
        <servlet-class>servlets.IniciaApp</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>EnviaConsumoFolios</servlet-name>
        <servlet-class>servlets.EnviaConsumoFolios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MuestraPath</servlet-name>
        <servlet-class>servlets.MuestraPath</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VaadinApplication</servlet-name>
        <url-pattern>/app/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VaadinApplication</servlet-name>
        <url-pattern>/VAADIN/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TraePDF</servlet-name>
        <url-pattern>/TraePDF</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IniciaApp</servlet-name>
        <url-pattern>/IniciaApp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnviaConsumoFolios</servlet-name>
        <url-pattern>/EnviaConsumoFolios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MuestraPath</servlet-name>
        <url-pattern>/MuestraPath</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file/>
    </welcome-file-list>
</web-app>
